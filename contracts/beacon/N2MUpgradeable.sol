/// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.18;

import "../interfaces/IN2MBeaconFactory.sol";
import "../proxy/Proxy.sol";
import "../N2MVersion.sol";

abstract contract N2MUpgradeable is Proxy {
    address internal immutable FACTORY_ADDRESS;

    constructor(address factoryAddress) {
        FACTORY_ADDRESS = factoryAddress;
    }

    function n2mVersions() external view returns (uint256 implementationVersion, uint256 beaconVersion) {
        implementationVersion = N2MVersion(address(this)).n2mVersion();
        beaconVersion = 1000;
    }
}
